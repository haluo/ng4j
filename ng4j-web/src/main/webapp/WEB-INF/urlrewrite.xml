<?xml version="1.0" encoding="UTF-8"?>
<urlrewrite default-match-type="wildcard">
    <rule>
        <from>/**</from>
        <to>/app/$1</to>
    </rule>
    <rule>
        <from>/app/favicon.ico</from>
        <to>/favicon.ico</to>
    </rule>
    <rule>
        <from>/app/temp/**</from>
        <to>/temp/$1</to>
    </rule>
    <rule>
        <from>/app/images/**</from>
        <to>/images/$1</to>
    </rule>
    <rule>
        <from>/app/js/**</from>
        <to>/js/$1</to>
    </rule>
    <rule>
        <from>/app/css/**</from>
        <to>/css/$1</to>
    </rule>
    <rule>
        <from>/app/img/**</from>
        <to>/img/$1</to>
    </rule>
    <rule>
        <from>/app/static/**</from>
        <to>/static/$1</to>
    </rule>
    <rule>
        <from>/app/html/**</from>
        <to>/html/$1</to>
    </rule>
    <outbound-rule>
        <from>/app/**</from>
        <to>/$1</to>
    </outbound-rule>
</urlrewrite>